<h2>Contact requests</h2>
<table class='table'>
<thead>
  <td>Name</td>
  <td>Email Address</td>
  <td>Telephone</td>
  <td>Message</td>
  <td>Created at</td>
</thead>
<tbody></tbody>
  <% @contact_requests.each do |cr |%>
    <tr>
        <td><%= cr.name %></td>
        <td><%= cr.email_address %></td>
        <td><%= cr.telephone %></td>
        <td><%= cr.comments %></td>
        <td><%= cr.created_at %></td>
    </tr>
  <% end%>
</table>