<h2>Contact requests</h2>
<table class='table'>
  <thead>
    <tr>
      <th><%= t('public.contact_us.table.headings.name') %></th>
      <th><%= t('public.contact_us.table.headings.email') %></th>
      <th><%= t('public.contact_us.table.headings.telephone') %></th>
      <th><%= t('public.contact_us.table.headings.comments') %></th>
      <th><%= t('public.contact_us.table.headings.created_at') %></th>
    </tr>
  </thead>
  <tbody>
    <% @contact_requests.each do |cr |%>
      <tr>
          <td><%= cr.name %></td>
          <td><%= cr.email_address %></td>
          <td><%= cr.telephone %></td>
          <td><%= cr.comments %></td>
          <td><%= cr.created_at %></td>
      </tr>
    <% end%>
  </tbody>
</table>