<h2>Contact requests</h2>
<table class='table'>
<thead>
  <td><% t('public.contact_us.table.headings.name') %></td>
  <td><% t('public.contact_us.table.headings.email') %></td>
  <td><% t('public.contact_us.table.headings.telephone') %></td>
  <td><% t('public.contact_us.table.headings.comments') %></td>
  <td><% t('public.contact_us.table.headings.created_at') %></td>
</thead>
<tbody></tbody>
  <% @contact_requests.each do |cr |%>
    <tr>
        <td><%= cr.name %></td>
        <td><%= cr.email_address %></td>
        <td><%= cr.telephone %></td>
        <td><%= cr.comments %></td>
        <td><%= cr.created_at %></td>
    </tr>
  <% end%>
</table>